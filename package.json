    "@babel/preset-env": "^7.13.10",
    "babel-plugin-bin-shebang": "^1.0.3",
    "eslint": "^7.21.0",
    "eslint-plugin-jest": "^24.2.1",
    "jest": "^26.6.3"
  },
  "dependencies": {
    "apollo-server": "^2.21.1",
    "commander": "^7.1.0",
